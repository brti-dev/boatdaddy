{
  "name": "boatdaddy",
  "version": "0.1.1",
  "description": "Hail boat daddy",
  "private": false,
  "license": "ISC",
  "main": "pages/_app.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "#clean": "Clean up dist files generated on dev, old builds, etc; Optimally done before new builds",
    "clean": "npx rimraf .next out",
    "#codegen": "Create types for mutations",
    "codegen": "npx apollo client:codegen --target typescript --localSchemaFile schema.gql --outputFlat --includes \"{pages,src}/**\" --excludes \"src/graphql/generated/**\" src/graphql/generated",
    "db:cli": "heroku pg:psql postgresql-parallel-93526 --app boatdaddy-db",
    "db:killconnections": "heroku pg:killall postgresql-parallel-93526 --app boatdaddy-db"
  },
  "dependencies": {
    "@apollo/client": "^3.4.8",
    "@prisma/client": "^2.27.0",
    "@reach/alert": "^0.15.0",
    "@reach/dialog": "^0.15.0",
    "@reach/menu-button": "^0.15.3",
    "@reach/skip-nav": "^0.15.0",
    "@reach/tooltip": "^0.13.2",
    "@reach/visually-hidden": "^0.15.0",
    "apollo-server-micro": "^3.1.2",
    "class-validator": "^0.13.1",
    "cloudinary": "^1.26.3",
    "cloudinary-react": "^1.7.0",
    "date-fns": "^2.23.0",
    "graphql": "^15.5.1",
    "micro": "^9.3.4",
    "next": "10.2.0",
    "next-auth": "^3.27.2",
    "normalize.css": "^8.0.1",
    "react": "17.0.2",
    "react-content-loader": "^6.0.3",
    "react-dom": "17.0.2",
    "react-icons": "^4.2.0",
    "react-map-gl": "^6.1.16",
    "reflect-metadata": "^0.1.13",
    "sass": "^1.32.8",
    "swr": "^0.5.6",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@types/node": "^15.14.1",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "babel-plugin-parameter-decorator": "^1.0.16",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "open": "^8.0.4",
    "prisma": "^2.27.0",
    "typescript": "^4.2.4"
  }
}
